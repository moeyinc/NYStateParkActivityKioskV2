{
  "name": "moey-activity-kiosk",
  "version": "1.0.0",
  "description": "Moey Activity Kiosk",
  "author": "Seiya Kobayashi",
  "private": true,
  "main": "electron-entry.js",
  "build": {
    "appId": "nuxt-electron",
    "directories": {
      "output": "build"
    }
  },
  "scripts": {
    "dev": "NODE_ENV=development nuxt",
    "build": "nuxt build",
    "start": "nuxt start",
    "generate": "nuxt generate",
    "lint": "eslint --ext .js,.vue --ignore-path .gitignore .",
    "lint-fix": "eslint --fix --ext .js,.vue --ignore-path .gitignore .",
    "electron:dev": "cross-env NODE_ENV=development BUILD_TARGET=electron electron .",
    "electron:build": "cross-env NODE_ENV=production BUILD_TARGET=electron nuxt build && electron-builder",
    "electron": "electron ."
  },
  "dependencies": {
    "dotenv": "^8.2.0",
    "nuxt": "^2.10.2"
  },
  "devDependencies": {
    "@nuxtjs/axios": "^5.8.0",
    "@nuxtjs/eslint-config": "^1.0.1",
    "@nuxtjs/eslint-module": "^1.0.0",
    "babel-eslint": "^10.0.1",
    "cross-env": "^6.0.3",
    "electron": "^7.1.3",
    "electron-builder": "^21.2.0",
    "electron-devtools-installer": "^2.2.4",
    "eslint": "^6.1.0",
    "eslint-plugin-nuxt": ">=0.4.2",
    "path": "^0.12.7",
    "reset-css": "^4.0.1",
    "stylus": "^0.54.7",
    "stylus-loader": "^3.0.2",
    "vue-lightbox": "^1.1.6",
    "vue-simple-svg": "^2.0.1",
    "vue-svg-loader": "^0.12.0"
  }
}
